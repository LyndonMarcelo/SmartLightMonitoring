<!DOCTYPE html>

<html>
<head>
    <title>SmartLight - @ViewData["Title"]</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    
    <!--favicons-->
    <link href="~/lib/favicon/favicon-32x32.png" rel="shortcut icon" type="image/x-icon" />
 

    <!--css files & google fonts-->
    <link href="~/css/home.css" rel="stylesheet" asp-append-version="true" />
    <link href="~/css/about.css" rel="stylesheet" asp-append-version="true" />
    <link href="~/css/login-style.css" rel="stylesheet" asp-append-version="true" />
    <link href="~/css/site.css" rel="stylesheet" type="text/css" asp-append-version="true" />
    <link href="~/css/nav.css" rel="stylesheet" type="text/css" asp-append-version="true" />
    <link href="~/css/power-style.css" rel="stylesheet" type="text/css" asp-append-version="true" />
    <link rel="stylesheet" href="~/LightSensorWebApp.styles.css" asp-append-version="true" />
    

    <style>
        @@import url('https://fonts.googleapis.com/css2?family=Montserrat:ital,wght@0,200;0,400;0,500;0,600;0,800;0,900;1,200&family=Playfair+Display:ital,wght@0,500;0,600;1,400;1,500&display=swap');
    </style>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.3.1/dist/css/bootstrap.min.css" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">

    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/LightSensorWebApp.styles.css" asp-append-version="true" />

</head>
<body>
    <div>

            @RenderSection("header")

    </div>

    <div>
        @RenderBody()
    </div>
</body>

<script src="nav-script.js " type="text/javascript "></script>


<script src="~/lib/jquery/dist/jquery.min.js" asp-append-version="true"></script>
<script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js" asp-append-version="true"></script>
<script src="~/js/site.js" asp-append-version="true"></script>
<script src="~/js/login-script.js" asp-append-version="true"></script>

</html>
